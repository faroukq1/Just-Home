{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "amount": {
      "type": "decimal",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "failed"
      ]
    },
    "transaction": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::transaction.transaction",
      "inversedBy": "payments"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "payments"
    }
  }
}
