    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "properties"
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "country": {
      "type": "string",
      "required": true
    },
    "features": {
      "type": "json"
    }
  }
